{
    "coordinationSpace": {
        "dataset": {
            "A": "A"
        },
        "spatialCellsLayer": 
            {
            "is_visible": {"opacity": 1,"radius": 50,"visible": true,"stroked": false},
            "is_not_visible": {"opacity": 1,"radius": 50,"visible": false,"stroked": false}
            }
    },
    "datasets": [
        {
            "files": [
                {
                    "type": "cells",
                    "fileType": "anndata-cells.zarr",
                    "url": "<DATA_FILE_URL>",
                    "options": {
                        "xy": "obsm/spatial"
                    }
                },
                {
                    "type": "expression-matrix",
                    "fileType": "anndata-expression-matrix.zarr",
                    "url": "<DATA_FILE_URL>",
                    "options": {
                        "matrix": "X"
                    }
                },
                {
                    "fileType": "raster.json",
                    "options": {
                        "images": [
                            {
                                "name": "<IMAGE_NAME>",
                                "type": "ome-tiff",
                                "url": "<IMAGE_URL>"
                            }
                        ],
                        "schemaVersion": "0.0.2",
                        "usePhysicalSizeScaling": false
                    },
                    "type": "raster"
                }
            ],
            "name": "Visualization Files",
            "uid": "A"
        }
    ],
    "description": "<DATASET_INFO>",
    "initStrategy": "auto",
    "layout": [
        {
            "component": "description",
            "coordinationScopes": {
                "dataset": "A"
            },
            "props":
            {
              "closeButtonVisible": false
            },
            "h": 3,
            "w": 4,
            "x": 0,
            "y": 0
        },
        {
            "component": "spatial",
            "coordinationScopes": {
                "spatialCellsLayer": "is_visible",
                "dataset": "A"
            },
            "h": 12,
            "w": 4,
            "x": 4,
            "y": 0
        },
        {
            "component": "spatial",
            "coordinationScopes": {
                "spatialCellsLayer": "is_not_visible",
                "dataset": "A"
            },
            "props":
            {
              "closeButtonVisible": false,
              "downloadButtonVisible": false
            },
            
            "h": 12,
            "w": 4,
            "x": 8,
            "y": 0
        },
        {
            "component": "genes",
            "x": 0,
            "y": 3,
            "w": 4,
            "h": 5
        },
        {
            "component": "expressionHistogram",
            "x": 0,
            "y": 7,
            "w": 4,
            "h": 4
        }
    ],
    "name": "KPMP Spatial Viewer",
    "version": "1.0.4"
}